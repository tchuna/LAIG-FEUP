<yas>
    <scene root="living_room" axis_length="5"/>
    <views default="south_view">
      <perspective id="south_view" near="0.1" far="40" angle="25">
          <from x="20" y="20" z="20"/>
          <to x="0" y="0" z="0"/>
      </perspective>

        <perspective id="east_view" near="0.1" far="40" angle="45">
            <from x="-20" y="20" z="0"/>
            <to x="0" y="0" z="0"/>
        </perspective>

        <perspective id="north_view" near="0.1" far="40" angle="45">
            <from x="0" y="20" z="20"/>
            <to x="0" y="0" z="0"/>
        </perspective>

        <perspective id="west_view" near="0.1" far="40" angle="45">
            <from x="20" y="20" z="0"/>
            <to x="0" y="0" z="0"/>
        </perspective>

        <perspective id="top_view" near="0.1" far="40" angle="90">
            <from x="0" y="10" z="-0.1"/>
            <to x="0" y="0" z="0"/>
        </perspective>

        <!--<ortho id="ortho_view" near="0.1" far="5" left="-2" right="2" top="2" bottom="-2">-->
        <!--<from x="4.1" y="3" z="1.8"/>-->
        <!--<to x="4.1" y="4" z="50"/>-->
        <!--</ortho>-->

        <!--<ortho id="painting_view" near="0.1" far="5" left="-3" right="3" top="2" bottom="-2">-->
        <!--<from x="2" y="3" z="4"/>-->
        <!--<to x="0" y="3" z="4"/>-->
        <!--</ortho>-->

        <!--<ortho id="cara_duarte" near="0.1" far="5" left="-2" right="2" top="1" bottom="-1">-->
        <!--<from x="4" y="3.45" z="2"/>-->
        <!--<to x="4" y="3.45" z="0"/>-->
        <!--</ortho>-->
    </views>

    <ambient>
        <ambient r="0" g="0" b="0" a="1"/>
        <background r="0.53" g="0.81" b="0.92" a="1" />
    </ambient>

    <lights>
        <omni id="back_1" enabled="0">
            <location x="1" y="12" z="1" w="0" />
            <ambient r="0.3" g="0.3" b="0.3" a="1"/>
            <diffuse r="0.3" g="0.3" b="0.3" a="1"/>
            <specular r="0.3" g="0.3" b="0.3" a="1"/>
        </omni>

        <omni id="back_2" enabled="0">
            <location x="7" y="12" z="1" w="0" />
            <ambient r="0.3" g="0.3" b="0.3" a="1"/>
            <diffuse r="0.3" g="0.3" b="0.3" a="1"/>
            <specular r="0.3" g="0.3" b="0.3" a="1"/>
        </omni>

        <omni id="front_1" enabled="0">
            <location x="1" y="12" z="7" w="0" />
            <ambient r="0.3" g="0.3" b="0.3" a="1"/>
            <diffuse r="0.3" g="0.3" b="0.3" a="1"/>
            <specular r="0.3" g="0.3" b="0.3" a="1"/>
        </omni>

        <omni id="front_2" enabled="0">
            <location x="7" y="12" z="7" w="0" />
            <ambient r="0.3" g="0.3" b="0.3" a="1"/>
            <diffuse r="0.3" g="0.3" b="0.3" a="1"/>
            <specular r="0.3" g="0.3" b="0.3" a="1"/>
        </omni>

        <spot id="center" enabled="1" angle="0" exponent="10">
            <location x="4" y="10" z="4" w="0" />
            <target x="4" y="0" z="4" />
            <ambient r="0.5" g="0.5" b="0.5" a="1.0"/>
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0"/>
            <specular r="0.5" g="0.5" b="0.5" a="1.0"/>
        </spot>

        <spot id="spot_light" enabled="1" angle="14" exponent="1">
            <location x="15" y="5" z="15" w="0" />
            <target x="0" y="0" z="0" />
            <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
            <diffuse r="0.2" g="0.2" b="0.2" a="1.0"/>
            <specular r="0.2" g="0.2" b="0.2" a="1.0"/>
        </spot>

    </lights>

    <textures>
        <texture id="white_wall" file="scenes/images//floor.png"/>
        <texture id="floor" file="scenes/images/wall3.jpeg"/>
        <texture id="brick_wall" file="scenes/images/tap2.jpg"/>
        <texture id="desk" file="scenes/images/desk.jpg"/>
        <texture id="metal" file="scenes/images/metal.jpeg"/>
        <texture id="leather" file="scenes/images/leather.jpeg"/>
        <texture id="def_text" file="scenes/images/default-texture.png"/>
        <texture id="starry_night" file="scenes/images/t2.jpg"/>
        <texture id="wood_grain" file="scenes/images/cwood.pjg"/>

    </textures>

    <materials>
        <material id="m1" shininess="0.1">
            <emission r="0" g="0" b="0" a="0"/>
            <ambient r="0.3" g="0.3" b="0.3" a="1"/>
            <diffuse r="0.8" g="0.8" b="0.8" a="1"/>
            <specular r="0.1" g="0.1" b="0.1" a="1"/>
        </material>

        <material id="floor" shininess="10">
            <emission r="0" g="0" b="0" a="0"/>
            <ambient r="1" g="1" b="1" a="1"/>
            <diffuse r="1" g="1" b="1" a="1"/>
            <specular r="0.1" g="0.1" b="0.1" a="1"/>
        </material>

        <material id="full_white" shininess="10">
            <emission r="0" g="0" b="0" a="0"/>
            <ambient r="1" g="1" b="1" a="1"/>
            <diffuse r="1" g="1" b="1" a="1"/>
            <specular r="1" g="1" b="1" a="1"/>
        </material>
        <material id="light_orange" shininess="10">
            <emission r="0" g="0" b="0" a="0"/>
            <ambient r="1" g="0.65" b="0.29" a="1"/>
            <diffuse r="1" g="0.65" b="0.29" a="1"/>
            <specular r="1" g="0.65" b="0.29" a="1"/>
        </material>

        <material id="fuschia" shininess="10">
            <emission r="0" g="0" b="0" a="0"/>
            <ambient r="1" g="0" b="0.3" a="1"/>
            <diffuse r="1" g="0" b="0.3" a="1"/>
            <specular r="1" g="0" b="0.3" a="1"/>
        </material>

        <material id="beige" shininess="10">
            <emission r="0" g="0" b="0" a="0"/>
            <ambient r="0.98" g="0.98" b="0.87" a="1"/>
            <diffuse r="0.98" g="0.98" b="0.87" a="1"/>
            <specular r="0" g="0" b="0" a="1"/>
        </material>

        <material id="table_wood" shininess="1">
            <emission r="0" g="0" b="0" a="0"/>
            <ambient r="1" g="1" b="1" a="1"/>
            <diffuse r="1" g="1" b="1" a="1"/>
            <specular r="1" g="1" b="1" a="1"/>
        </material>

        <material id="lamp" shininess="1">
            <emission r="0" g="0" b="0" a="0"/>
            <ambient r="1" g="1" b="1" a="1"/>
            <diffuse r="1" g="1" b="1" a="1"/>
            <specular r="1" g="1" b="1" a="1"/>
        </material>

        <material id="silver" shininess="1000">
            <emission r="0" g="0" b="0" a="0"/>
            <ambient r="0.83" g="0.83" b="0.83" a="1"/>
            <diffuse r="0.83" g="0.83" b="0.83" a="1"/>
            <specular r="0.83" g="0.83" b="0.83" a="1"/>
        </material>
    </materials>


    <transformations >
        <transformation id="floor_trans">
            <translate x="0" y="0" z="8"/>
            <rotate axis="x" angle="-90"/>
            <scale x="8" y="8" z="1"/>
        </transformation>
    </transformations>



    <primitives>
        <primitive id="square">
            <rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />
        </primitive>
        <primitive id="cylinder">
          <cylinder base="1" top="1" height="5" slices="250" stacks="250"/>
        </primitive>

        <primitive id="square_at_zero">
            <rectangle x1="0" y1="0" x2="1" y2="1" />
        </primitive>

        <primitive id="triangle">
            <triangle x1="0" y1="1" z1="0"
                    x2="-0.8" y2="0" z2="0"
                    x3="0.8" y3="0" z3="0"/>
        </primitive>

    </primitives>

    <components>

        <component id="left_wall">
            <transformation>
                <translate x="0" y="0" z="8"/>
                <rotate axis="y" angle="-180"/>
                <rotate axis="y" angle="-90"/>
                <scale x="8" y="5" z="1"/>
            </transformation>

            <materials>
                <material id ="full_white"/>
            </materials>

            <texture id="white_wall" length_s="1" length_t="1"/>

            <children>
                <primitiveref id="square_at_zero"/>
            </children>
        </component>


        <component id="right_wall">
            <transformation>
                <translate x="8" y="0" z="8"/>
                <rotate axis="y" angle="-180"/>
                <rotate axis="y" angle="-90"/>
                <scale x="8" y="5" z="1"/>
            </transformation>

            <materials>
                <material id ="full_white"/>
            </materials>

            <texture id="white_wall" length_s="1" length_t="1"/>

            <children>
                <primitiveref id="square_at_zero"/>
            </children>
        </component>



        <component id="back_wall">
            <transformation>
                <scale x="8" y="5" z="1"/>
            </transformation>

            <materials>
                <material id ="beige"/>
            </materials>

            <texture id="white_wall" length_s="1" length_t="1"/>

            <children>
                <primitiveref id="square_at_zero"/>
            </children>
        </component>

        <component id="floor">
            <transformation>
                <transformationref id="floor_trans" />
            </transformation>

            <materials>
                <material id ="floor"/>
            </materials>

            <texture id="floor" length_s="1" length_t="1"/>

            <children>
                <primitiveref id="square_at_zero"/>
            </children>
        </component>

        <component id="floor_1">
            <transformation>
                <transformationref id="floor_trans" />
                <scale x="0.5" y="0.5" z="0.5"/>
                <translate x="0.55" y="0.5" z="0.003"/>
            </transformation>

            <materials>
                <material id ="floor"/>
            </materials>

            <texture id="brick_wall" length_s="1" length_t="1"/>

            <children>
                <primitiveref id="square_at_zero"/>
            </children>
        </component>

        <component id="frontal_face">
            <transformation>
                <translate x="0" y="0" z="0.5"/>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit"/>

            <children>
                <primitiveref id="square"/>
            </children>
        </component>

        <component id="back_face">
            <transformation>
                <rotate axis="x" angle="180"/>
                <translate x="0" y="0" z="0.5"/>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit"/>

            <children>
                <primitiveref id="square"/>
            </children>

        </component>

        <component id="top_face">
            <transformation>
                <rotate axis="x" angle="-90"/>
                <translate x="0" y="0" z="0.5"/>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit"/>

            <children>
                <primitiveref id="square"/>
            </children>
        </component>

        <component id="bottom_face">
            <transformation>
                <rotate axis="x" angle="90"/>
                <translate x="0" y="0" z="0.5"/>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit"/>

            <children>
                <primitiveref id="square"/>
            </children>
        </component>

        <component id="right_face">
            <transformation>
                 <rotate axis="y" angle="90"/>
                <translate x="0" y="0" z="0.5"/>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit"/>

            <children>
                <primitiveref id="square"/>
            </children>
        </component>


        <component id="left_face">
            <transformation>
                <rotate axis="y" angle="-90"/>
                <translate x="0" y="0" z="0.5"/>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit" />

            <children>
                <primitiveref id="square"/>
            </children>
        </component>

        <component id="cube">
            <transformation>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit"/>

            <children>
                <componentref id="frontal_face"/>
                <componentref id="back_face"/>
                <componentref id="bottom_face"/>
                <componentref id="left_face"/>
                <componentref id="right_face"/>
                <componentref id="top_face"/>
            </children>
        </component>


        <component id="table_top">
            <transformation>
                <translate x="0" y="3.65" z="0.0"/>
                <scale x="5" y="0.3" z="3"/>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit"/>

            <children>
                <componentref id="cube"/>
            </children>
        </component>

        <component id="front_right_leg">
            <transformation>
                <translate x="-2.35" y="1.75" z="1.35"/>
                <scale x="0.3" y="3.5" z="0.3"/>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit" length_s="1" length_t="1"/>

            <children>
                <componentref id="cube"/>
            </children>
        </component>

        <component id="front_left_leg">
            <transformation>
                <translate x="2.35" y="1.75" z="1.35"/>
                <scale x="0.3" y="3.5" z="0.3"/>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit" length_s="1" length_t="1"/>

            <children>
                <componentref id="cube"/>
            </children>
        </component>

        <component id="back_left_leg">
            <transformation>
                <translate x="2.35" y="1.75" z="-1.35"/>
                <scale x="0.3" y="3.5" z="0.3"/>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit" length_s="1" length_t="1"/>

            <children>
                <componentref id="cube"/>
            </children>
        </component>

        <component id="back_right_leg">
            <transformation>
                <translate x="-2.35" y="1.75" z="-1.35"/>
                <scale x="0.3" y="3.5" z="0.3"/>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit" length_s="1" length_t="1"/>

            <children>
                <componentref id="cube"/>
            </children>
        </component>

        <component id="legs">
            <transformation>
            </transformation>

            <materials>
                <material id ="inherit"/>
            </materials>

            <texture id="inherit" length_s="1" length_t="1"/>

            <children>
                <componentref id="front_right_leg"/>
                <componentref id="front_left_leg"/>
                <componentref id="back_left_leg"/>
                <componentref id="back_right_leg"/>
            </children>
        </component>


        <component id="table">
            <transformation>


            </transformation>

            <materials>
                <material id ="table_wood"/>
            </materials>

            <texture id="inherit"/>

            <children>
                <componentref id="legs"/>
                <componentref id="table_top"/>

            </children>
        </component>

        <component id="desk">
            <transformation>
                <translate x="4.2" y="0" z="4"/>
                <scale x="0.6" y="0.2" z="0.8"/>
            </transformation>

            <materials>
                <material id ="table_wood"/>
            </materials>

            <texture id="desk" length_s="1" length_t="1"/>

            <children>
                <componentref id="table"/>
            </children>
        </component>
        
        <component id="canvas">
           <transformation>
               <scale x="3" y="2" z="0.15"/>

           </transformation>

           <materials>
               <material id="inherit"/>
           </materials>

           <texture id="inherit" length_s="1" length_t="1"/>

           <children>
               <primitiveref id="square"/>
           </children>

       </component>

       <component id="left_frame">
           <transformation>
               <translate x="-1.5" y="0" z="0"/>
               <scale x="0.2" y="2.4" z="0.3"/>

           </transformation>

           <materials>
               <material id="inherit"/>
           </materials>

           <texture id="inherit" length_s="1" length_t="1"/>

           <children>
               <componentref id="cube"/>
           </children>

       </component>

       <component id="right_frame">
           <transformation>
               <translate x="1.5" y="0" z="0"/>
               <scale x="0.2" y="2.4" z="0.3"/>
           </transformation>

           <materials>
               <material id="inherit"/>
           </materials>

           <texture id="inherit" length_s="1" length_t="1"/>

           <children>
               <componentref id="cube"/>
           </children>

       </component>

       <component id="top_frame">
           <transformation>
               <translate x="0" y="1.1" z="0"/>
               <scale x="2.8" y="0.2" z="0.3"/>
           </transformation>

           <materials>
               <material id="inherit"/>
           </materials>

           <texture id="inherit" length_s="1" length_t="1"/>

           <children>
               <componentref id="cube"/>
           </children>

       </component>

       <component id="bot_frame">
           <transformation>
               <translate x="0" y="-1.1" z="0"/>
               <scale x="2.8" y="0.2" z="0.3"/>
           </transformation>

           <materials>
               <material id="inherit"/>
           </materials>

           <texture id="inherit" length_s="1" length_t="1"/>

           <children>
               <componentref id="cube"/>
           </children>

       </component>

        <component id="frame">
            <transformation>

            </transformation>

            <materials>
                <material id="inherit"/>
            </materials>

            <texture id="def_text" length_s="1" length_t="1"/>

            <children>
                <componentref id="left_frame"/>
                <componentref id="right_frame"/>
                <componentref id="top_frame"/>
                <componentref id="bot_frame"/>
            </children>

        </component>

        <component id="starry_night">
           <transformation>
               <translate x="0.2" y="2.8" z="4"/>
               <rotate axis="y" angle="90"/>
               <scale x="2" y="1" z="0.3"/>

           </transformation>

           <materials>
               <material id="table_wood"/>
           </materials>

           <texture id="starry_night" length_s="1" length_t="1"/>

           <children>
               <componentref id="canvas"/>
               <componentref id="frame"/>
           </children>

       </component>

       <component id="chair">
           <transformation>
               <translate x="4.2" y="0.3" z="6"/>
               <scale x="1" y="0.6" z="0.8"/>
           </transformation>

           <materials>
               <material id ="table_wood"/>
           </materials>

           <texture id="def_text" length_s="1" length_t="1"/>

           <children>
               <componentref id="cube"/>
           </children>
       </component>

       <component id="chair_2">
           <transformation>
               <translate x="4.2" y="0.3" z="2"/>
               <scale x="1" y="0.6" z="0.8"/>
           </transformation>

           <materials>
               <material id ="table_wood"/>
           </materials>

           <texture id="def_text" length_s="1" length_t="1"/>

           <children>
               <componentref id="cube"/>
           </children>
       </component>

        <component id="living_room">
            <transformation>
            </transformation>

            <materials>
                <material id ="m1"/>
            </materials>

            <texture id="none"/>

            <children>
                <componentref id="desk"/>
                <componentref id="left_wall"/>
                <componentref id="back_wall"/>
                <componentref id="floor"/>
                <componentref id="chair"/>
                <componentref id="chair_2"/>

                <componentref id="starry_night"/>
            </children>
        </component>



    </components>


</yas>
