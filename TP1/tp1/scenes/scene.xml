<yas>

  <scene root="root" axis_length="1.0" />

  <views default="persp">
    <perspective id="persp" near="0.1" far="1000" angle="0.0">
      <from x="0.0" y="0.0" z="0.0" />
      <to x="50.0" y="50.0" z="50.0" />
    </perspective>

    <ortho id="ortho" near="0.1" far="1000"
                      left="25.0" right="25.0"
                      top="25.0" bottom="25.0" />
  </views>

  <ambient>
    <ambient r="1.0" g="1.0" b="1.0" a="1.0"/>
    <background r="1.0" g="1.0" b="1.0" a="1.0" />
  </ambient>

  <lights>
    <omni id="light0" enable="1">
      <location x="0.0" y="0.0" z="0.0" w="10.0" />
      <ambient r="1.0" g="0.0" b="0.0" a="1.0" />
      <diffuse r="1.0" g="0.0" b="0.0" a="1.0" />
      <specular r="1.0" g="0.0" b="0.0" a="1.0" />
    </omni>
  </lights>

  <textures>
    <texture id="texture0" file="/images/leaves.jpg" />
  </textures>

  <materials>
    <material id="material0" shininess="1.0">
      <emission r="1.0" g="1.0" b="1.0" a="1.0" />
      <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </material>
  </materials>

  <transformations>
    <transformation id="transform0">
      <translate x="0.0" y="0.0" z="0.0" />
      <rotate axis="x" angle="0.0" />
      <scale x="1.0" y="1.0" z="1.0" />
    </transformation>
  </transformations>

  <primitives>
    <primitive id="square">
      <rectangle x1="0.0" y1="0.0" x2="1.0" y2="1.0" />
    </primitive>
  </primitives>
</yas>
